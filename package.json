
{
  "name": "mythos",
  "version": "1.0.0",
  "description": "This is an online social media platform for Live Action Role Players. ",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "node server && npm install && cd client && npm run build && npm start",
    "heroku-postbuild": "npm install && node server && cd client && npm run build"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkcodes/Mythos.git"
  },
  "author": "Will Kendall, Du'Ryan Ebbesen, Josh Payne",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wkcodes/Mythos/issues"
  },
  "homepage": "https://github.com/wkcodes/Mythos#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.0",
    "express-handlebars": "^5.1.0",
    "express-session": "^1.17.1",
    "merge-images": "^2.0.0",
    "mysql2": "^1.6.5",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^5.8.6"
  }
}
